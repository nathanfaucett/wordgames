{
	"name": "wordgames",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"tauri": "tauri",
		"vite": "vite",
		"svelte-kit": "svelte-kit",
		"clean": "rm -rf node_modules/.vite .svelte-kit build",
		"start": "vite dev --host --open",
		"build": "vite build",
		"preview": "vite preview",
		"desktop": "tauri dev",
		"desktop:build": "tauri build",
		"android": "tauri android dev --open",
		"android:build": "tauri android build",
		"ios": "tauri ios dev --open",
		"ios:build": "tauri ios build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.15.9",
		"@tauri-apps/api": "^2.0.0-alpha.3",
		"@tauri-apps/cli": "^2.0.0-alpha.8",
		"@types/howler": "^2.2.7",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"internal-ip": "^7.0.0",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"socket.io-client": "^4.6.1",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@aicacia/graph": "^0.1.1",
		"@aicacia/mesh": "^0.1.1",
		"@aicacia/rand": "^0.2.14",
		"@aicacia/range": "^0.1.4",
		"howler": "^2.2.3",
		"svelte-localstorage-writable": "^0.1.1",
		"uuid": "^9.0.0"
	}
}
